  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.9.3",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
